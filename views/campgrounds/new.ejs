<%- include('../partials/header'); -%>
<div class="container">
	<div class="row">
		<h1 style="text-align: center">Create a new entry for CampGroung</h1>
		<div style="width: 30%; margin:25px auto;">
			<form action="/campgrounds" method="POST">
				<div class="form-group">
					<input class="form-control" type="text" name="name" placeholder="enter Camp name">	
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="location" placeholder="Location">
				</div>
				<div class="form-group">
					<input class="form-control" type="number" name="prize" placeholder="prize" min="0" step="1">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="image" placeholder="enter img url">
				</div>
				<div class="form-group">
					<input class="form-control" type="text" name="discription" placeholder="enter discription">	
				</div>
				<div class="form-group">
					<button class="btn btn-lg btn-primary btn-block">Submit!</button>
				</div>	
			</form>
			<a href="/">Go Back</a>
		</div>
		
	</div>
</div>
	
<%- include('../partials/footer'); -%>